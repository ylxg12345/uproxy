{
  "manifest_version": 2,
  "name": "uProxy",
  "description": "Get and share access to the open internet",
  "minimum_chrome_version": "41.0.2272.63",
  "version": "0.0",
  "icons": {
      "128": "icons/128_online.png"
  },
  "app": {
    "background": {
      "scripts": [
        "freedom-for-chrome/freedom-for-chrome.js",
        "scripts/main.core-env.static.js"
      ]
    }
  },
  "permissions": [
    "identity",
    "storage",
    "http://*/",
    "https://*/",
    "notifications",
    "proxy"
  ],
  "sockets": {
    "tcp": { "connect" : "" },
    "tcpServer": { "listen": "" },
    "udp": { "send": "", "bind": "" }
  },
  "externally_connectable": {
    "matches" : ["*://*.uproxy.org/*", "*://uproxysite.appspot.com/*", "*://test-dot-uproxysite.appspot.com/*", "*://beta-dot-uproxysite.appspot.com/*"]
  },
  "key": ""
}
